<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="CurrentProgramIncrement" verbose="1">
    <listeners>
        <listener class-name="repository.listeners.LocalSuiteTestListener"/>
    </listeners>

    <parameter name="application" value="Current Program Increment"/>
    <parameter name="suiteName" value="PI3-Regression-Clock"/>

	<test name="F276 BC Document Cleanup">
        <groups>
            <run>
                <include name="ClockMove"/>
            </run>
        </groups>
        <packages>
            <package name="previousProgramIncrement.pi3_090518_111518.f276_BC_Document_Cleanup.*"/>
            <package name="previousProgramIncrement.pi3_090518_111518.f276_BC_Document_Cleanup.*.*"/>
            <package name="previousProgramIncrement.pi3_090518_111518.f276_BC_Document_Cleanup.*.*.*"/>
        </packages>
    </test>
    
    <test name="Maintenance Defects - NoExceptions">
        <groups>
            <run>
                <include name="ClockMove"/>
            </run>
        </groups>
        <packages>
            <package name="previousProgramIncrement.pi3_090518_111518.maintenanceDefects.NoExceptions2.*"/>
        </packages>
    </test>

</suite>