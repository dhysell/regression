<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="CurrentProgramIncrement" verbose="1">
    <listeners>
        <listener class-name="repository.listeners.LocalSuiteTestListener"/>
    </listeners>

    <parameter name="application" value="Current Program Increment"/>
    <parameter name="suiteName" value="CurrentProgramIncrement-Clock"/>
    
    <test name="Non Feature Related Work - Scope Creeps">
        <groups>
            <run>
                <include name="ClockMove"/>
            </run>
        </groups>
        <packages>
            <package name="currentProgramIncrement.nonFeatures.ScopeCreeps.*"/>
        </packages>
    </test>
    
    <test name="Maintenance Defects - Scope Creeps">
        <groups>
            <run>
                <include name="ClockMove"/>
            </run>
        </groups>
        <packages>
            <package name="currentProgramIncrement.maintenanceDefects.ScopeCreeps.*"/>
        </packages>
    </test>
    
    <test name="Non Feature Related Work - Triton">
        <groups>
            <run>
                <include name="ClockMove"/>
            </run>
        </groups>
        <packages>
            <package name="currentProgramIncrement.nonFeatures.Triton.*"/>
        </packages>
    </test>
    
    <test name="Maintenance Defects - Triton">
        <groups>
            <run>
                <include name="ClockMove"/>
            </run>
        </groups>
        <packages>
            <package name="currentProgramIncrement.maintenanceDefects.Triton.*"/>
        </packages>
    </test>
    
    
    <test name="F349 APLUS Reports - Scope Creeps">
        <groups>
            <run>
                <include name="ClockMove"/>
            </run>
        </groups>
        <packages>
            <package name="currentProgramIncrement.f349_APLUSReports_ScopeCreeps.*"/>
            <package name="currentProgramIncrement.f349_APLUSReports_ScopeCreeps.*.*"/>
            <package name="currentProgramIncrement.f349_APLUSReports_ScopeCreeps.*.*.*"/>
        </packages>
    </test>
    
    <test name="F273 Activities Clean Up - Triton">
        <groups>
            <run>
                <include name="ClockMove"/>
            </run>
        </groups>
        <packages>
            <package name="currentProgramIncrement.f273_ActivitiesCleanUp_Triton.*"/>
            <package name="currentProgramIncrement.f273_ActivitiesCleanUp_Triton.*.*"/>
            <package name="currentProgramIncrement.f273_ActivitiesCleanUp_Triton.*.*.*"/>
        </packages>
    </test>
    
    <test name="F278 BillingCenter Payment Clean Up - Triton">
        <groups>
            <run>
                <include name="ClockMove"/>
            </run>
        </groups>
        <packages>
            <package name="currentProgramIncrement.f278_BillingCenterPaymentCleanup_Triton.*"/>
            <package name="currentProgramIncrement.f278_BillingCenterPaymentCleanup_Triton.*.*"/>
            <package name="currentProgramIncrement.f278_BillingCenterPaymentCleanup_Triton.*.*.*"/>
        </packages>
    </test>

</suite>